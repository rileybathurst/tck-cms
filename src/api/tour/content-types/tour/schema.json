{
  "kind": "collectionType",
  "collectionName": "tours",
  "info": {
    "singularName": "tour",
    "pluralName": "tours",
    "displayName": "tour",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "^[a-z0-9\\-]*$"
    },
    "information": {
      "type": "richtext"
    },
    "start": {
      "type": "time"
    },
    "finish": {
      "type": "time"
    },
    "peek": {
      "type": "string"
    },
    "price": {
      "type": "integer"
    },
    "minimum": {
      "type": "integer"
    },
    "duration": {
      "type": "decimal"
    },
    "fitness": {
      "type": "enumeration",
      "enum": [
        "easy",
        "moderate"
      ]
    },
    "sport": {
      "type": "enumeration",
      "enum": [
        "kayak",
        "sup"
      ]
    },
    "excerpt": {
      "type": "string",
      "maxLength": 160,
      "minLength": 120
    },
    "ogimage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
