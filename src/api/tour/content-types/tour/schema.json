    "slug": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "^[a-z0-9\\-]*$"
    },
    "information": {
      "type": "richtext"
    },
    "start": {
      "type": "time"
    },
    "finish": {
      "type": "time"
    }
  }
}
