{
  "kind": "collectionType",
  "collectionName": "tours",
  "info": {
    "singularName": "tour",
    "pluralName": "tours",
    "displayName": "tour"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
