{
  "kind": "collectionType",
  "collectionName": "brands",
  "info": {
    "singularName": "brand",
    "pluralName": "brands",
    "displayName": "brand",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "string",
      "unique": true,
      "required": true,
      "regex": "^[a-z0-9\\-]*$"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "tagline": {
      "type": "string"
    },
    "svg": {
      "type": "text"
    },
    "retail": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::retail.retail",
      "mappedBy": "brand"
    },
    "kayak": {
      "type": "boolean"
    },
    "sup": {
      "type": "boolean"
    },
    "svglogo": {
      "type": "richtext"
    }
  }
}
